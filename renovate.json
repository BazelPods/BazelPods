{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "prHourlyLimit": 10,
  "automerge": true,
  "dependencyDashboard": false,
  "labels": ["renovate"],
  "packageRules": [
    {
      "matchDepNames": ["rules_apple", "rules_swift"],
      "enabled": false
    },
    {
      "matchDepNames": ["rules_ios"],
      "enabled": true
    }
  ]
}